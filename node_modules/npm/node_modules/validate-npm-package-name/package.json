{
  "_id": "validate-npm-package-name@3.0.0",
  "_inBundle": true,
  "_location": "/npm/validate-npm-package-name",
  "_phantomChildren": {},
  "_requiredBy": [
    "/npm",
    "/npm/init-package-json",
    "/npm/npm-package-arg"
  ],
  "author": {
    "name": "zeke"
  },
  "bugs": {
    "url": "https://github.com/npm/validate-npm-package-name/issues"
  },
  "dependencies": {
    "builtins": "^1.0.3"
  },
  "description": "Give me a string and I'll tell you if it's a valid npm package name",
  "devDependencies": {
    "standard": "^8.6.0",
    "tap": "^10.0.0"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/npm/validate-npm-package-name",
  "keywords": [
    "npm",
    "package",
    "names",
    "validation"
  ],
  "license": "ISC",
  "main": "index.js",
  "name": "validate-npm-package-name",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npm/validate-npm-package-name.git"
  },
  "scripts": {
    "cov:test": "TAP_FLAGS='--cov' npm run test:code",
    "test": "npm run test:code && npm run test:style",
    "test:code": "tap ${TAP_FLAGS:-'--'} test/*.js",
    "test:style": "standard"
  },
  "version": "3.0.0"
}
